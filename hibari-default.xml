<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <remote  name="github"
           fetch="git://github.com/hibari/" />
  <default revision="rebarify"
           remote="github" />

  <!--
      Applications - Tools
  -->
  <project path="hibari/lib/cluster_info" name="cluster-info" />
  <project path="hibari/lib/congestion_watcher" name="congestion-watcher" />
  <project path="hibari/lib/gmt_bom" name="gmt-bom" />
  <project path="hibari/lib/gmt" name="gmt-util" />
  <project path="hibari/lib/partition_detector" name="partition-detector" />

  <!--
      Applications - UBF
  -->
  <project path="hibari/lib/ubf" name="ubf" />
  <project path="hibari/lib/ubf_jsonrpc" name="ubf-jsonrpc" />
  <project path="hibari/lib/ubf_thrift" name="ubf-thrift" />

  <!--
      Applications - Hibari
  -->
  <project path="hibari/lib/gdss" name="gdss-brick" />
  <project path="hibari/lib/gdss_client" name="gdss-client" />
  <project path="hibari/lib/gdss_admin" name="gdss-admin" />
  <project path="hibari/lib/gdss_json_rpc_proto" name="gdss-json-rpc-proto" />
  <project path="hibari/lib/gdss_s3_proto" name="gdss-s3-proto" />
  <project path="hibari/lib/gdss_ubf_proto" name="gdss-ubf-proto" />

  <!--
      Applications - Third party
  -->
  <project path="hibari/lib/mochiweb" name="mochiweb" />

  <!--
      Releases - Hibari
  -->
  <project path="hibari" name="hibari" />

  <!--
      Patches - Hibari
  -->
  <project path="patches" name="patches" />

  <!--
      Documentation - Hibari
  -->
  <project path="hibari-doc" name="hibari-doc"
           revision="new-user-guide" />

</manifest>
