<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <default remote="origin" revision="dev" />
  <remote name="origin" fetch="git@github.com:hibari/" />
  <remote name="h2leveldb" fetch="git@github.com:h2leveldb/" />

  <!--
      Applications - Tools
  -->
  <project path="hibari/lib/cluster_info" name="cluster-info" />
  <project path="hibari/lib/congestion_watcher" name="congestion-watcher" />
  <project path="hibari/lib/gmt_util" name="gmt-util" />
  <project path="hibari/lib/partition_detector" name="partition-detector" />

  <!--
      Applications - UBF
  -->
  <project path="hibari/lib/ubf" name="ubf" />
  <project path="hibari/lib/ubf_thrift" name="ubf-thrift" />
  <project path="hibari/lib/pmod_transform" name="pmod_transform" />

  <!--
      Applications - Hibari
  -->
  <project path="hibari/lib/gdss_brick" name="gdss-brick" />
  <project path="hibari/lib/gdss_client" name="gdss-client" />
  <project path="hibari/lib/gdss_admin" name="gdss-admin" />
  <project path="hibari/lib/gdss_ubf_proto" name="gdss-ubf-proto" />

  <!--
      Applications - Third party - HyperLevelDB
  -->
  <project path="hibari/lib/h2leveldb" name="h2leveldb" remote="h2leveldb" />
  <project path="hibari/lib/HyperLevelDB" name="HyperLevelDB" remote="h2leveldb" />
  <project path="hibari/lib/snappy" name="snappy" remote="h2leveldb" />

  <!--
      Applications - Third party - Monitoring
  -->
  <project path="hibari/lib/lager" name="lager" />
  <project path="hibari/lib/folsom" name="folsom" />
  <project path="hibari/lib/bear" name="bear" />

  <!--
      Applications - Third party - Test
  -->
  <project path="hibari/lib/meck" name="meck" />

  <!--
      Applications - Third party - Docs
  -->
  <project path="hibari/lib/edown" name="edown" />
  <project path="hibari/lib/asciiedoc" name="asciiedoc" />
  <project path="hibari/lib/qc" name="qc" />

  <!--
      Releases - Hibari
  -->
  <project path="hibari" name="hibari" />

  <!--
      Miscellaneous
  -->
  <project path="hibari-doc" name="hibari-doc" />
  <project path="patches" name="patches" />

</manifest>
